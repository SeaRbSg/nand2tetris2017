/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    Not(in=sel[0],out=notsel0);
    Not(in=sel[1],out=notsel1);
    And(a=notsel1,b=notsel0,out=channel0);
    And(a=notsel1,b=sel[0],out=channel1);
    And(a=sel[1],b=notsel0,out=channel2);
    And(a=sel[1],b=sel[0],out=channel3);
    And(a=channel0,b=in,out=a);
    And(a=channel1,b=in,out=b);
    And(a=channel2,b=in,out=c);
    And(a=channel3,b=in,out=d);
}
