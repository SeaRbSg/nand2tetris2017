CHIP MyMux {
  IN
    a, b, sel;
  OUT
    o,  // a if sel == 0, b otherwise
    no; // not(out)
  PARTS:
    /* PROBLEM
    Mux(a=a, b=a, sel=sel, out=o);
    Not(in=o, out=no);
//      ^^^^ ERROR: Can't connect gate's output pin to part
    */

    /* SOLUTION */
    Mux(a=a, b=b, sel=sel, out=o, out=temp);
    Not(in=temp, out=no);
}
