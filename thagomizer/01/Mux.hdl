/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);
    Nand(a=a, b=notsel, out=w1);
    Nand(a=b, b=sel, out=w2);
    Nand(a=w1, b=w2, out=out);
}
